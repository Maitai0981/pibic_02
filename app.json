{
  "expo": {
    "name": "PIBIC Dermatologia",
    "slug": "pibic-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pibic.dermatologia",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Este app precisa acessar suas fotos para análise dermatológica.",
        "NSCameraUsageDescription": "Este app precisa acessar a câmera para capturar imagens de lesões.",
        "NSPhotoLibraryAddUsageDescription": "Este app precisa salvar laudos na sua galeria."
      }
    },
    "android": {
      "package": "com.pibic.dermatologia",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "INTERNET",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-dev-client",
      "expo-font"
    ],
    "extra": {
      "apiBaseUrl": "http://172.25.153.3:5000",
      "apiProtocol": "http",
      "apiPort": "5000",
      "apiPrefix": "/api",
      "eas": {
        "projectId": "34cf275c-bac3-49b7-a7b8-7ac9741c475c"
      }
    },
    "runtimeVersion": "1.0.0",
    "owner": "math2091",
    "updates": {
      "url": "https://u.expo.dev/34cf275c-bac3-49b7-a7b8-7ac9741c475c"
    }
  }
}
